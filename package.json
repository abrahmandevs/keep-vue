{
  "type": "module",
  "name": "keep-vue",
  "version": "0.0.0",
  "description": "Official Vue components built for Vue and Tailwind CSS",
  "license": "MIT",
  "keywords": [
    "design-system",
    "keep-design-system",
    "keep-vue",
    "vue",
    "tailwind",
    "tailwindcss",
    "tailwind-css"
  ],
  "files": [
    "./lib/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "default": "./lib/cjs/index.js"
      }
    },
    "./keepTheme": {
      "types": "./lib/esm/themes/keepTheme.d.ts",
      "default": "./lib/cjs/themes/keepTheme.js"
    },
    "./utils": {
      "types": "./lib/esm/utils/cn.d.ts",
      "default": "./lib/cjs/utils/cn.js"
    },
    "./css": {
      "default": "./lib/cjs/themes/keepStyle.min.css"
    }
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "build:lib": "rm -rf lib && yarn build:lib:dev",
    "build:lib:dev": "yarn build:lib:esm && yarn build:lib:cjs && yarn build:css",
    "build:css": "css-minify -f ./styles/keepStyle.css -o ./lib/cjs/themes",
    "build:lib:cjs": "vue-tsc -p tsconfig.lib.json",
    "build:lib:esm": "vue-tsc -p tsconfig.lib.json --module esnext --declaration --outDir ./lib/esm"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.5",
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/image": "^1.7.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/mdc": "^0.7.1",
    "@phosphor-icons/vue": "^2.2.1",
    "@vueuse/core": "^11.0.1",
    "@vueuse/nuxt": "^11.0.1",
    "chart.js": "^4.4.3",
    "clsx": "^2.1.1",
    "deepmerge": "^4.3.1",
    "embla-carousel-autoplay": "^8.2.1",
    "embla-carousel-class-names": "^8.2.1",
    "embla-carousel-vue": "^8.2.1",
    "nuxt": "^3.11.1",
    "radix-vue": "^1.9.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "unhead": "^1.9.4",
    "vaul-vue": "^0.2.0",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.1",
    "vue-router": "^4.3.0",
    "vue-sonner": "^1.1.5",
    "vue3-dropzone": "^2.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "css-minify": "^2.0.0",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.5.4",
    "vue-tsc": "^2.1.6"
  }
}
